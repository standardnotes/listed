<% author_credential_url = author_credential_path(@author, @credential, :secret => @author.secret) %>

<%= react_component("CredentialsEdit", props: {
    author: @author.as_json(
        only: :display_name,
    ),
    authenticityToken: form_authenticity_token,
    authorCredentialUrl: author_credential_url,
    credential: @credential
}) %>
